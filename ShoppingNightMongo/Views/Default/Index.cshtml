@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_UIHeadComponent")
<body class="animsition">

    @await Component.InvokeAsync("_UINavbarComponent")

    @await Component.InvokeAsync("_UISliderComponent")

    @await Component.InvokeAsync("_UICategoryComponent")

    @await Component.InvokeAsync("_UIProductComponent")

    @await Html.PartialAsync("_FooterPartial")

    <!-- Back to top -->
    <div class="btn-back-to-top" id="myBtn">
        <span class="symbol-btn-back-to-top">
            <i class="zmdi zmdi-chevron-up"></i>
        </span>
    </div>

    @* Modal *@

    @await Component.InvokeAsync("_UIScriptComponent")   

</body>
</html>
